config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Project_Rose"
run/main_scene="res://Scenes/MainMenu/MainMenu.tscn"
config/icon="res://icon.png"

[autoload]

UserSettings="*res://user_settings.gd"
GameFlags="*res://game_flags.gd"
pixel_perfect_viewport="*res://pixel_perfect_viewport.gd"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=480
window/size/height=270
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "Palette Swap" )

